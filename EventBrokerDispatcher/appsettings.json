{
  "rabbitMQSetting": {
    "RabbitMQServer": "ubuntu01.kaiser.local",
    "RabbitMQUsername": "guest",
    "RabbitMQPassword": "guest",
    "RabbitMQVirtualHost": "/"
  },
  "eventBrokerQueues" : {
    "queues": [
      {
        "queueName": "CAM",
        "url": "https://camservice",
        "bindings": [
          {
            "senderId": "Serve.CAM.Events",
            "eventType": "CAM.Customer.Created"
          },
          {
            "senderId": "Serve.CAM.Events",
            "eventType": "CAM.Card.Created"
          }
        ]
      },
      {
        "queueName": "TXP",
        "url": "https://txpservice",
        "bindings": [
          {
            "senderId": "Serve.TXP.Events",
            "eventType": "TXP.Transaction.Complete"
          }
        ]
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Console":
    {
      "IncludeScopes": true
    },
    "Debug":
    {
      "IncludeScopes": true
    },
    "File": {
      "BasePath": "Logs",
      "EnsureBasePath": true,
      "FileEncoding": "utf-8",
      "FileNameMappings": {          
        "Default": "default.log"
      },
      "DateFormat": "yyyyMMdd",
      "CounterFormat": "000",
      "MaxFileSize": 10485760,                
      "IncludeScopes": true,
      "MaxQueueSize": 100
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://ubuntu01.kaiser.local:9200/",
    "Application": "EventBroker.Dispatcher"
  }
}